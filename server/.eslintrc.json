{
    "env": {
        "commonjs": false,
        "es6": true,
        "node": true
    },
    "extends": [
        "prettier",
        "airbnb-base"
    ],
    "plugins": [
        "prettier"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly",
        "utils": "writable",
        "mongoose": "writable",
        "message": "writable",
        "push_message": "writable",
        "User": "writable",
        "Setting": "writable",
        "EmailSetup": "writable",
        "SmsSetup": "writable",
        "Schema": "writable",
        "FOLDER_NAME": "writable",
        "DEGREE_TO_KM": "writable",
        "DEVICE_TYPE": "writable",
        "ADDRESS_TYPE": "writable",
        "SETTLED_BY": "writable",
        "USER_TYPE": "writable",
        "ADMIN_PROFIT": "writable",
        "WALLET_STATUS": "writable",
        "WALLET_STTAUS_ID": "writable",
        "REQUEST_STATUS": "writable",
        "REQUEST_STATUS_ID": "writable",
        "API_TYPE": "writable",
        "PAYMENT_TYPE": "writable",
        "PAYMENT_GATEWAY_LIST": "writable",
        "SMS_UNIQUE_ID": "writable",
        "REQUEST_TYPE": "writable",
        "DATE_FORMATE": "writable",
        "ERROR_CODE": "writable",
        "MESSAGE_CODE": "writable",
        "PUSH_CODE": "writable",
        "EMAIL_UNIQUE_IDS": "writable",
        "FILE_EXTENSION": "writable",
        "EMAIL_TEMPLATES": "writable",
        "LOGIN_TYPES": "writable",
        "IMAGE_URL_TYPE": "writable",
        "UserEvent": "writable",
        "CM_MEMBER_TYPE": "writable",
        "Community": "writable",
        "CommunityMember": "writable",
        "CLIENT_DEVICE_TYPE": "writable",
        "Invitation": "writable",
        "EVENT_STATUS": "writable",
        "INV_TYPE": "writable",
        "FileUpload": "writable",
        "Discussion": "writable",
        "ChatRecord": "writable",
        "Tag": "writable",
        "DUPLICATE_TYPE": "writable",
        "MainTag": "writable",
        "ChatMsg": "writable",
        "CHAT_MESSAGE_TYPE": "writable",
        "UserInfoForm": "writable",
        "UserInfoData": "writable",
        "GOOGLE_HOLIDAY_COUNTRY_LIST": "writable",
        "Holiday": "writable"
    },
    "parserOptions": {
        "ecmaVersion": 2018
    },
    "rules": {
        "linebreak-style": 0,
        "indent": "off",
        "no-console": "off",
        "camelcase": "off",
        "max-len": "off",
        "func-names": "off",
        "no-unused-vars": "off",
        "no-param-reassign": "off",
        "no-tabs": "off",
        "no-underscore-dangle": "off",
        "object-curly-newline": "off",
        "eqeqeq": "warn"
    }
}